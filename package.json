        "serve": "^14.2.5",
        "xml-formatter": "^3.6.7",
        "zustand": "^5.0.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.35.0",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.19.17",
        "esbuild": "^0.25.10",
        "eslint": "^9.36.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "globals": "^16.4.0",
        "jsdom": "^27.0.0",
        "lodash-es": "^4.17.21",
